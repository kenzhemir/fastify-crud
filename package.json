{
  "name": "@kenzhemir/fastify-crud",
  "version": "0.1.2",
  "description": "Fastify plugin for creating CRUD routes",
  "main": "bundle.cjs",
  "module": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./bundle.cjs"
    }
  },
  "types": "types/index.d.ts",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "tap run",
    "test:mutation": "stryker run",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kenzhemir/fastify-crud.git"
  },
  "keywords": [
    "fastify",
    "crud"
  ],
  "author": "Miras Kenzhegaliyev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenzhemir/fastify-crud/issues"
  },
  "homepage": "https://github.com/kenzhemir/fastify-crud#readme",
  "volta": {
    "node": "20.12.2",
    "npm": "10.6.0"
  },
  "devDependencies": {
    "@stryker-mutator/tap-runner": "^8.2.6",
    "fastify": "^4.26.2",
    "prettier": "3.2.5",
    "rollup": "^4.17.2",
    "standard": "*",
    "tap": "^18.7.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
